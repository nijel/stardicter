{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices",
    ":dependencyDashboard",
    "customManagers:githubActionsVersions"
  ],
  "automerge": true,
  "automergeType": "pr",
  "automergeStrategy": "rebase",
  "platformAutomerge": true,
  "git-submodules": {
    "enabled": true
  },
  "pre-commit": {
    "enabled": true
  }
}
